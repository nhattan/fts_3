<div class="span5 offset1">
    <div><strong>Supervisor in course</strong></div>
    <%= will_paginate @user_admin_enrolls %>
    <% @user_admin_enrolls.each do |user| %>
    	<div>
      <%= link_to user.name, user %>
      <%= link_to 'Remove', admin_course_supervisor_path(@course, id: user.id), method: :delete %>
      </div>
    <% end %>

    <div><strong>Add supervisor in course</strong></div>
    <%= will_paginate @user_admin_not_enrolls %>
      <div>
        
          <% @user_admin_not_enrolls.each do |user| %>
            <div>
            	<%= link_to user.name, user %>
            	<%= link_to 'Add', admin_course_supervisors_path(@course.id, id: user.id), method: :create %>              
            </div>
          <% end %>
      </div>
  </div>
</div>